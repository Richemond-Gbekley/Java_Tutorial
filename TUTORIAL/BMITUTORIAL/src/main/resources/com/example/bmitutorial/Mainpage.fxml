<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bmitutorial.Mainpage">
   <children>
      <TextField fx:id="name" layoutX="151.0" layoutY="67.0" />
      <TextField fx:id="Weight" layoutX="152.0" layoutY="153.0" />
      <TextField fx:id="Height" layoutX="153.0" layoutY="247.0" />
      <TextField fx:id="bmiRESULTS" layoutX="382.0" layoutY="67.0" promptText="BMI RESULTS" />
      <TextField fx:id="Remarks" layoutX="383.0" layoutY="153.0" promptText="REMARKS" />
      <Button fx:id="Calbmi" layoutX="431.0" layoutY="212.0" mnemonicParsing="false" onAction="#Calbmi" text="Calculate BMI" />
      <Button fx:id="save" layoutX="357.0" layoutY="309.0" mnemonicParsing="false" onAction="#save" text="Save" />
      <Button fx:id="go" layoutX="507.0" layoutY="309.0" mnemonicParsing="false" onAction="#go" text="GO" />
      <Button fx:id="GoBack" layoutX="191.0" layoutY="309.0" mnemonicParsing="false" onAction="#goback" text="Go Back" />
      <Label layoutX="52.0" layoutY="71.0" text="Name" />
      <Label layoutX="52.0" layoutY="157.0" text="Weight" />
      <Label layoutX="52.0" layoutY="251.0" text="Height" />
